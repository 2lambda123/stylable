{
  "name": "@stylable/webpack-extensions",
  "version": "2.0.0",
  "description": "Experimental Stylable webpack plugins",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "scripts": {
    "clean": "rimraf ./cjs ./esm",
    "build": "ts-build ./src --cjs --esm",
    "lint": "run-p lint:src lint:test",
    "lint:src": "tslint -p ./src",
    "lint:test": "tslint -p ./test",
    "test": "mocha -r @ts-tools/node/r \"test/**/*.spec.ts?(x)\" --watch-extensions ts,tsx --timeout 20000",
    "prepack": "yarn build"
  },
  "dependencies": {
    "@stylable/core": "^2.0.0",
    "lodash.clonedeep": "^4.5.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "webpack": "^4.29.6",
    "webpack-sources": "^1.3.0"
  },
  "files": [
    "cjs",
    "esm",
    "src",
    "!src/tsconfig.json"
  ],
  "engines": {
    "node": ">=8"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/wix/stylable/tree/master/packages/webpack-extensions",
  "author": "Wix.com",
  "license": "BSD-3-Clause",
  "keywords": [
    "css",
    "stylable",
    "webpack",
    "webpack-plugin"
  ]
}
